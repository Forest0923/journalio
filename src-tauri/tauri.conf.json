{
  "$schema": "https://schema.tauri.app/config/1",
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "JournalIO",
  "mainBinaryName": "journalio",
  "version": "1.0.2",
  "identifier": "com.journalio.app",
  "plugins": {
    "fs": {
      "requireLiteralLeadingDot": false
    }
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "JournalIO",
        "width": 1400,
        "minWidth": 1353,
        "height": 800,
        "minHeight": 600
      }
    ]
  }
}
